<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
 <changeSet id="014_insert_terrains" author="jessthepig">
        <insert tableName="terrains">
            <column name="name" value="Plains"/>
            <column name="description" value="Open, grassy terrain with few obstructions."/>
            <column name="date_created" valueDate="NOW()"/>
            <column name="last_updated" valueDate="NOW()"/>
        </insert>

        <insert tableName="terrains">
            <column name="name" value="Forest"/>
